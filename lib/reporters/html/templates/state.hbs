<div class="meta-info meta-info_collapsed">
    <div class="meta-info__switcher">Meta-info</div>
    <div class="meta-info__content">
        <pre>{{metaInfo}}</pre>
    </div>
</div>
{{#if error}}
    <div class="image-box__image">
        <pre class="reason">{{reason}}</pre>
        {{#if image}}
            {{image "actual"}}
        {{/if}}
    </div>
{{/if}}
{{#if warning}}
    <div class="image-box__image">
        <div class="image-box__title">New image</div>
        {{image "actual"}}
        <pre class="reason">{{reason}}</pre>
    </div>
{{/if}}
{{#if success}}
    <div class="image-box__image">
        {{image "expected"}}
    </div>
{{/if}}
{{#if fail}}
    <div class="image-box__exp-with-act">
        <div class="image-box__image">
            <div class="image-box__title">Expected</div>
            {{image "expected"}}
        </div>
        <div class="image-box__image">
            <div class="image-box__title">Actual</div>
            {{image "actual"}}
        </div>
    </div>
    <div class="image-box__image">
        <div class="image-box__title">Diff</div>
        {{image "diff"}}
    </div>
{{/if}}
